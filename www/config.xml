<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sdp1" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SDP1</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <!--<access origin="*" />-->
    <access origin="http://students.engr.scu.edu/~kdedoshk" subdomains="true"/>
    <feature name="EmailComposer">
        <param name="ios-package" value="APPEmailComposer" />
    </feature>
    <gap:plugin name="de.appplant.cordova.plugin.email-composer" />
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <gap:plugin name="com.phonegap.plugins.emailcomposer" version="1.3" />
    
</widget>
